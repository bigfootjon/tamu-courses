irmovl Stack, %esp

irmovl $0, %eax # j = 0
irmovl $0, %ecx # k = 0
# ...
irmovl $0, %edx # i = 0

Loop:

# j = i * 2
rrmovl %edx, %ebx
addl %ebx, %ebx
rrmovl %ebx, %eax

# k = j + 1
irmovl $1, %ebx
addl %eax, %ebx
rrmovl %ebx, %ecx

# for loop calcs:
irmovl $1, %ebx
addl %ebx, %edx
irmovl $5, %ebx
subl %edx, %ebx
jne Loop

halt

Stack:
